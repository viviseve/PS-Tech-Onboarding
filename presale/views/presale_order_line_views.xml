<?xml version="1.0"?>
<odoo>
    <record id="presale_order_line_view_tree" model="ir.ui.view">
        <field name="name">presale.order.line.tree</field>
        <field name="model">presale.order.line</field>
        <field name="arch" type="xml">
            <tree string="Presales Order Lines" editable="top">
                <field name="product_id"/>
                <field name="quantity"/>
                <field name="price"/>
            </tree>
        </field>
    </record>

    <record id="presale_order_line_action" model="ir.actions.act_window">
        <field name="name">Presale Order Line</field>
        <field name="res_model">presale.order.line</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
