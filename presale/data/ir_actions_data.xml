<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="presale_order_line_action" model="ir.actions.act_window">
        <field name="name">Presale Order Line</field>
        <field name="res_model">presale.order.line</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="presale_order_action" model="ir.actions.act_window">
        <field name="name">Presales Orders</field>
        <field name="res_model">presale.order</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="sale_order_action" model="ir.actions.act_window">
        <field name="name">Sales Orders</field>
        <field name="res_model">sale.order</field>
        <field name="view_mode">tree</field>
        <field name="domain">[('presale_id', '=', active_id)]</field>
    </record>
</odoo>
